{
    "name": "dees040/repository",
    "description": "A Laravel package for the repository pattern.",
    "keywords": [
        "Laravel",
        "Package",
        "Repository",
        "Pattern",
        "Eloquent"
    ],
    "authors": [
        {
            "name": "Dees Oomens",
            "email": "deesoomens@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|6.*|7.*|8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*|7.0.*|8.0.*|9.0.*",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*",
        "mockery/mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Dees040\\Repository\\": "src/Repository"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dees040\\Repository\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dees040\\Repository\\ServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
